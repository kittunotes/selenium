#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  --harmony-generators "$basedir/../image2pdf/bin/image2pdf.js" "$@"
  ret=$?
else 
  node  --harmony-generators "$basedir/../image2pdf/bin/image2pdf.js" "$@"
  ret=$?
fi
exit $ret
